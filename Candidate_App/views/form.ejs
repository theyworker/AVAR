    <div class="flexslider js-fullheight">

      <div class="container" style="padding-top: 100px;">
<div id="msg1"></div>
        <div class="page-header">
          <h1>Application for <%= job %> </h1>
        </div>
        <form class="col-md-12" name="Appform" action="/submit" method="POST" encType="multipart/form-data">
          <div class="row">
            <div class="col-md-12">
              <div class="col-md-6">
                <div id="biodata">
                  <div >
                    <h2>Bio Data</h2>
                  </div>
                  <div class="form-group">
                    <label for="fname">Full Name:</label>
                    <input type="text" class="form-control" name="fname" id="fname">
                     
                  </div>
                  <div class="form-group">
                    <label for="email">Email:</label>
                    <input type="email" class="form-control" name="email" id="email">
                    <div id="msg2"></div>
                  </div>
                  <div class="form-group">
                    <label for="tel">Telephone Number:</label>
                    <input type="tel" class="form-control" name="tel" id="tel">
                    <div id="msg3"></div>
                  </div>
                  <div class="form-group">
                    <label for="address">Address:</label>
                    <textarea type="text" class="form-control" name="address" id="address"></textarea>
                  </div>
                </div>
              </div>
              <div class="col-md-6">
                <div>
                  <h2>Current Employment</h2>
                </div>
                <div class="form-group">
                  <label for="curemp">Current Designation:</label>
                  <input type="text" class="form-control" name="curemp" id="curemp">
                </div>
                <div class="form-group">
                  <label for="curind">Current Industry:</label>
                  <input type="text" class="form-control" name="curind" id="curind">
                </div>
                <div class="col-md-6" style="padding-left: 0;">
                  <div >
                    <h2>Academics</h2>
                  </div>
                  <div class="form-group">
                    <label for="quali">Highest Qualification :</label>
                    <select class="form-control formstyle" style="width: 250px;"  name="quali" id="quali">
                      <option>--Select Education--</option>
                      <option>Certificate</option>
                      <option>Diploma</option>
                      <option>Higher Diploma</option>
                      <option>Bachelor</option>
                      <option>Master's Degree</option>
                      <option>phD</option>
                    </select>
                    <!-- <input type="text" class="form-control">-->
                  </div>
                </div>
                <div class="col-md-6" style="padding: 0;">
                  <div >
                    <h2>Experience</h2>
                  </div>
                  <div >
                    <label for="quali">Years of experience :</label>
                    <input type="number" class="form-control" name="demo" id="demo">
                  </div>
                </div>
                <br>

              </div>
              <div class="col-md-6">
                <div class="form-group">
                  <label for="curind">Salary Scale:</label>
                  <select class="form-control formstyle"  name="quali" id="quali">
                    <option>--Select Salary Range--</option>
                    <option>30,000 - 50,000</option>
                    <option>50,000 - 80,000</option>
                    <option>80,000- 100,000</option>
                    <option>100,000 - 150,000</option>
                    <option>150,000 - 250,000</option>
                    <option>250,000 - 400,000</option>
                    <option>400,000 - 600,000</option>
                    <option>600,000 - 1,000,000</option>
                    <option>1,000,000 - 2,000,000</option>
                    <option>2,000,000 - 3,500,000</option>
                    <option>3,500,000+</option>
                  </select>
                </div>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-12">
              <div class="col-md-6">
                <div class="form-group">
                  <label for="curind">LinkedIn profile(URL)</label>
                  <input type="text" class="form-control" name="Lurl" id="Lurl">
                </div>
              </div>
              <div class="col-md-6">
                <div>
                  <br/>
                  <h2>Upload your updated CV </h2><input type="file" name="cvfile">

                </div>
                <br/>
                <input class="btn btn-success btn-outline pull-right" type="submit" value="Submit" onclick="validation()"; style="height: 45px;margin-bottom: 30px; line-height: 0;">
              </div>
            </div>

          </div>
        </form>



